{% extends 'basedash.html.twig' %}

{% block title %}
    Admin | Rajouter une compétence
{% endblock %}

{% block body %}
<div class="container-fluid">
  <h1>Rajouter une compétence</h1>
    {{ form_start(form, {attr: {class: 'form-group'} }) }}
        {{ form_row(form.name, {attr: {class: 'form-control-lg'} }) }}
        {{ form_row(form.image, {attr: {class: 'form-control-file'} }) }}
        {{ form_row(form.save, {attr: {class: 'btn btn-lg btn-success'}}) }}
    {{ form_end(form) }}
</div>
{% endblock %}
